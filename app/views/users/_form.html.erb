
<%if @user.errors.any?%>
    <%@user.errors.full_messages.each do |msg|%>
    <p><%= msg%></p>
<%end%>
<%end%>




<%=form_for(@user) do |f|%>
 <%=f.label :first_name%>
 <%=f.text_field :first_name%>
 <%=f.label :last_name%>
 <%=f.text_field :last_name%>
 <%=f.label :user_name%>
 <%=f.text_field :user_name%>
 <%=f.label :password%>
 <%=f.password_field :password%>
 <%=f.submit%>
 <%end%>