<div class="content-wrapper">
<h1 class="title-text">Pick your is like artists</h1>
<div class="form-wrapper">
<%= form_for(@is_like) do |f| %>
    <div class="dropdown">
    <div class="like-selector">
    <%= f.label :liker_id %>
    <%= f.collection_select :liker_id, Artist.all, :id, :name, {prompt: "Choose a liker"} %>
    </div>
    <div class="like-selector">
    <%= f.label :liked_id %>
    
    <%= f.collection_select :liked_id, Artist.all, :id, :name, {prompt: "Choose who they are like"} %>
    </div>
    </div>
    <br>
    <br>
    <div class="test"> 
    <%= f.submit "Submit", class:"button" %>
    </div>

<% end %>
</div>
</div>